[
    {
        "name": "welcome",
        "name_localizations": {
            "es-ES": "${es-ES:welcomeName}"
        },
        "description": "${es-ES:welcomeDescription}", 
        "description_localizations": {
            "es-ES": "${es-ES:welcomeDescription}"
        }, 
        "options": [
            {
                "type": "${constants:Subcommand}",
                "name": "settextmessage",
                "name_localizations": {
                    "es-ES": "${es-ES:settextmessageName}"
                },
                "description": "${es-ES:setwelcome}",
                "options": [
                    {
                        "type": "${constants:String}",
                        "name": "text",
                        "name_localizations": {
                            "es-ES": "${es-ES:textName}"
                        },
                        "description": "${es-ES:welcomeMessage}",
                        "description_localizations": {
                            "es-ES": "${es-ES:welcomeMessage}"
                        },
                        "required": true
                    }
                ]
            },
            {
                "type": "${constants:Subcommand}",
                "name": "enableimage",
                "name_localizations": {
                    "es-ES": "${es-ES:enableimageName}"
                },
                "description": "${es-ES:welcomeMessageImageDesc}",
                "description_localizations": {
                    "es-ES": "${es-ES:welcomeMessageImageDesc}"
                },
                "options": [
                    {
                        "type": "${constants:Boolean}",
                        "name": "useimage",
                        "name_localizations": {
                            "es-ES": "${es-ES:useimageName}"
                        },
                        "description": "${es-ES:welcomeMessageImageActive}",
                        "description_localizations": {
                            "es-ES": "${es-ES:welcomeMessageImageActive}"
                        },
                        "required": true
                    }
                ]
            }
        ]
    }
]