[
    {
        "name": "welcome",
        "name_localizations": {
            "es-ES": "${es-ES:welcomeName}",
            "en-US": "${en-US:welcomeName}",
            "en-GB": "${en-US:welcomeName}",
            "fr": "${fr:welcomeName}"
        },
        "description": "${es-ES:welcomeDescription}", 
        "description_localizations": {
            "es-ES": "${es-ES:welcomeDescription}",
            "en-US": "${en-US:welcomeDescription}",
            "en-GB": "${en-US:welcomeDescription}"
        }, 
        "options": [
            {
                "type": "${constants:Subcommand}",
                "name": "settextmessage",
                "name_localizations": {
                    "es-ES": "${es-ES:settextmessageName}",
                    "en-US": "${en-US:settextmessageName}",
                    "en-GB": "${en-US:settextmessageName}"
                },
                "description": "${es-ES:setwelcome}",
                "options": [
                    {
                        "type": "${constants:String}",
                        "name": "text",
                        "name_localizations": {
                            "es-ES": "${es-ES:textName}",
                            "en-US": "${en-US:textName}",
                            "en-GB": "${en-US:textName}"
                        },
                        "description": "${es-ES:welcomeMessage}",
                        "description_localizations": {
                            "es-ES": "${es-ES:welcomeMessage}",
                            "en-US": "${en-US:welcomeMessage}",
                            "en-GB": "${en-US:welcomeMessage}"
