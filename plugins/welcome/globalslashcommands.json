                "options": [
                    {
                        "type": "${constants:Boolean}",
                        "name": "useimage",
                        "name_localizations": {
                            "es-ES": "${es-ES:useimageName}",
                            "en-US": "${en-US:useimageName}",
                            "en-GB": "${en-US:useimageName}"
                        },
                        "description": "${es-ES:welcomeMessageImageActive}",
                        "description_localizations": {
                            "es-ES": "${es-ES:welcomeMessageImageActive}",
                            "en-US": "${en-US:welcomeMessageImageActive}",
                            "en-GB": "${en-US:welcomeMessageImageActive}"
                        },
                        "required": true
                    }
                ]
            }
